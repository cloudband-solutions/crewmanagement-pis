<table class="table table-bordered table-stripped">
  <tr>  
    <th class="label-c">License Type</th>
    <th class="label-c">Number</th>
    <th class="label-c">Date Issued</th>
    <th class="label-c">Expiry Date</th>
    <th class="label-c">Issued By</th>
    <th class="label-c">Actions</th>
  </tr>
  <% licenses.each do |license| %>
    <tr>
      <td class="label-d-a"><%= license.license_type %></td>
      <td class="label-d-a"><%= license.license_number %></td>
      <td class="label-d-a"><%= license.date_issued %></td>
      <td class="label-d-a"><%= license.expiry_date %></td>
      <td class="label-d-a"><%= license.issued_by %></td>
      <td class="label-d-a">
        <% if license.attachment.exists? %>
          <%= link_to "Download", license.attachment.url, target: "_blank" %>
        <% end %>
      </td>
    </tr>
  <% end %>
</table>

