<div class="for_non_mobile">
  <div class="row details">
    <div class="col-md-12">
      <table class="table table-bordered table-stripped">
        <tr>
          <td class="label-a">
            Vessel Assigned
          </td>
          <td class="label-b">
            <%= crew.vessel %>
          </td>
           <td class="label-a">
            Age
          </td>
          <td class="label-b">
            <%= crew.age %>
          </td>
        </tr>
        <tr>
          <td class="label-a">
            Home Address
          </td>
          <td class="label-b">
            <%= crew.address %>
          </td>
           <td class="label-a">
            City Address
          </td>
          <td class="label-b">
            <%= crew.city_address %>
          </td>
        </tr>
        <tr>  
          <td class="label-a">
            Provincial Address
          </td>
          <td class="label-b">
            <%= crew.provincial_address %>
          </td>

           <td class="label-a">
            Zipcode
          </td>
          <td class="label-b">
            <%= crew.zip_code %>
          </td>
        </tr>
        <tr>
          <td class="label-a">
             Birthday
          </td>
          <td class="label-b">
           <%= crew.birthday.strftime('%d-%B-%Y') %>
          </td>
          <td class="label-a">
           Birth Place
          </td>
          <td class="label-b">
            <%= crew.birthplace %>
          </td>
        </tr>
        <tr>
         <td class="label-a">
            Telephone
          </td>
          <td class="label-b">
          <%= crew.telephone_no %>
          </td>
          <td class="label-a">
           Cellphone
          </td>
          <td class="label-b">
            <%= crew.cellphone_no %>
          </td>
        </tr>
      </table>
       <table class="table table-bordered table-stripped">
         <tr>
          <td class="label-a">
            Civil Status
          </td>
          <td class="label-b">
            <%= crew.civil_status %>
